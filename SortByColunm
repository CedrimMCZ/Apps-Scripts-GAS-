function onOpen() {

  SpreadsheetApp.getUi().createMenu('ðŸ…¼ðŸ…´ðŸ…½ðŸ†„')
    .addItem('Classificar', 'classificar')
    .addToUi();

  classificar();
}


function classificar() {

  var spreadsheet = SpreadsheetApp.getActive();
  var sheet = spreadsheet.getActiveSheet();
    sheet.getRange(1, 1, sheet.getMaxRows(), sheet.getMaxColumns()).activate();
    spreadsheet.getActiveRange().offset(1, 0, spreadsheet.getActiveRange().getNumRows() - 1).sort([{ column: 15, ascending: true }, { column: 2, ascending: true }]);
    spreadsheet.getRange('A1').activate();
};
